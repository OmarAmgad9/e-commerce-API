{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",  
        "use": "@vercel/node", 
        "config": {
          "distDir": "dist" 
        }
      },
      {
        "src": "/**/*", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",  
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",  
        "dest": "/uploads/products/$1"
      }
    ]
  }
  